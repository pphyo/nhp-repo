	<bean id="emailService" class="com.pphyo.exwiring.beans.EmailService" />
	<bean id="popService" class="com.pphyo.exwiring.beans.PopService" />
	
	<bean id="appService" class="com.pphyo.exwiring.beans.AppService" autowire="byName"></bean>

